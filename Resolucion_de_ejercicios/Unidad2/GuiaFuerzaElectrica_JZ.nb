    RowBox[{"D", "[", 
     RowBox[{
      FractionBox[
       SuperscriptBox["x", "2"], "b"], ",", "x"}], "]"}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x", "\[Rule]", "px"}], ",", 
      RowBox[{"b", "\[Rule]", "1"}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mn", "=", 
   RowBox[{
    RowBox[{"-", 

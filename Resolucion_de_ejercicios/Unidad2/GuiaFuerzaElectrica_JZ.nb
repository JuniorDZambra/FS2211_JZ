Cell[TextData[{
 StyleBox["(5a) ",
  FontWeight->"Bold"],
 "La fuerza el\[EAcute]ctrica sobre la carga negativa, debido al resto de \
cargas positivas.\n",
 StyleBox["(5b)",
  FontWeight->"Bold"],
 " Determine la fuerza el\[EAcute]ctrica que act\[UAcute]a sobre la carga \
negativa debido a una carga positiva de valor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"4", "q"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ubicada en el centro de masa de la distribuci\[OAcute]n discreta de cargas \
positiva."
}], "Text",
 CellChangeTimes->{{3.833903282092123*^9, 3.833903348371888*^9}}],

Cell[TextData[StyleBox["Soluci\[OAcute]n:",
 FontWeight->"Bold",
 Background->RGBColor[0.87, 0.94, 1]]], "Text",
 CellChangeTimes->{3.8338007734988766`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Posici\[OAcute]n", " ", "de", " ", "las", " ", "cargas", " ", 
     "positivas", " ", "y", " ", "negativa"}], ",", " ", "respectivamente"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rq1", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "R"}], ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq2", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "R"}], " ", 
        RowBox[{"Cos", "[", 
         FractionBox["\[Pi]", "3"], "]"}]}], ",", 
       RowBox[{"R", " ", 
        RowBox[{"Sin", "[", 
         FractionBox["\[Pi]", "3"], "]"}]}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq3", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"R", " ", 
        RowBox[{"Cos", "[", 
         FractionBox["\[Pi]", "3"], "]"}]}], ",", 
       RowBox[{"R", " ", 
        RowBox[{"Sin", "[", 
         FractionBox["\[Pi]", "3"], "]"}]}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq4", "=", 
     RowBox[{"{", 
      RowBox[{"R", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "R"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<(5a) Fuerza el\[EAcute]ctrica sobre la carga negativa\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{"R", ">", "0"}], ",", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"Refine", "[", 
       RowBox[{
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], "q"}], ",", "rq", ",", "q", ",", "rq1"}], 
         "]"}], "+", 
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], "q"}], ",", "rq", ",", "q", ",", "rq2"}], 
         "]"}], "+", 
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], "q"}], ",", "rq", ",", "q", ",", "rq3"}], 
         "]"}], "+", 
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], "q"}], ",", "rq", ",", "q", ",", "rq4"}], 
         "]"}]}], "]"}], "]"}]}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Posici\[OAcute]n", " ", "del", " ", "centro", " ", "de", " ", "masa", 
     " ", "de", " ", "la", " ", "distribuci\[OAcute]n", " ", "discreta", " ", 
     "de", " ", "cargas"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rcm", "=", 
     FractionBox[
      RowBox[{"rq1", "+", "rq2", "+", "rq3", "+", "rq4"}], "4"]}], ";"}], 
   "\[IndentingNewLine]", 
   "\"\<(5b) Fuerza el\[EAcute]ctrica sobre la carga negativa debido a la \
carga especificada\>\"", "\[IndentingNewLine]", 
   RowBox[{"Cancel", "[", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{"R", ">", "0"}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"Refine", "[", 
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], "q"}], ",", "rq", ",", 
          RowBox[{"4", "q"}], ",", "rcm"}], "]"}], "]"}], "]"}]}], "]"}], 
    "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{
     "rq1", ",", "rq2", ",", "rq3", ",", "rq4", ",", "rq", ",", "rcm"}], 
     "]"}], ";"}]}]}]], "Input",
 CellChangeTimes->{{3.833903567789583*^9, 3.8339039618195095`*^9}, {
   3.8339039975665145`*^9, 3.833904186677618*^9}, 3.834253297319687*^9}],

Cell[BoxData["\<\"(5a) Fuerza el\[EAcute]ctrica sobre la carga \
negativa\"\>"], "Output",
 CellChangeTimes->{{3.8339038176465483`*^9, 3.8339039032901125`*^9}, {
   3.8339039626632977`*^9, 3.833903982726435*^9}, {3.833904164023408*^9, 
   3.8339041878095655`*^9}, 3.8339895188457537`*^9}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{
      SqrtBox["2"], " ", 
      RowBox[{"(", 
       RowBox[{"2", "+", 
        SqrtBox["3"]}], ")"}], " ", 
      SuperscriptBox["q", "2"], " ", 
      SubscriptBox["K", "0"]}], 
     SuperscriptBox["R", "2"]]}]}], "}"}]], "Output",
 CellChangeTimes->{{3.8339038176465483`*^9, 3.8339039032901125`*^9}, {

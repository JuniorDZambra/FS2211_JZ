Cell[TextData[{
 StyleBox["(2a)",
  FontWeight->"Bold"],
 " La fuerza el\[EAcute]ctrica que ejerce el semi-aro sobre la carga ubicada \
en el origen."
}], "Text",
 CellChangeTimes->{{3.833370402063489*^9, 3.833370425135044*^9}},
 TextJustification->1.],

Cell[TextData[{
 "Calculamos la fuerza el\[EAcute]ctrica que ejerce la distribuci\[OAcute]n \
de carga continua sobre la carga ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". Ponemos el centro del sistema de coordenadas en la posici\[OAcute]n de ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y, en base a los datos presentados por la imagen del problema, definimos \
la posici\[OAcute]n del diferencial de carga ",
 Cell[BoxData[
  FormBox["dq", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " parametrizando la curva del semi-aro:"
}], "Text",
 CellChangeTimes->{{3.833370505380849*^9, 3.833370614212715*^9}, {
  3.8333706593326883`*^9, 3.8333707660581636`*^9}},
 TextJustification->1.],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Refine", "[", 
   RowBox[{
    RowBox[{"Fe", "[", 
     RowBox[{
      SubscriptBox["Q", "0"], ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0"}], "}"}], ",", "dq", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"R", " ", 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
        RowBox[{"R", " ", 
         RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], "}"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"R", ">", "0"}], ",", 
      RowBox[{"0", "\[LessEqual]", "\[Theta]", "\[LessEqual]", 
       FractionBox["\[Pi]", "2"]}]}], "}"}]}], "]"}], "//", 
  "FullSimplify"}]], "Input",
 CellChangeTimes->{{3.8333707753264074`*^9, 3.8333708164041357`*^9}, {
  3.8333708578695755`*^9, 3.833370946971392*^9}, {3.8333710776167707`*^9, 
  3.8333711214004526`*^9}, {3.8333711661936097`*^9, 3.833371167523981*^9}, {
  3.8333712025294333`*^9, 3.8333712449984283`*^9}, {3.833371554035534*^9, 
  3.83337155483106*^9}, {3.83337179806273*^9, 3.833371801081917*^9}, {
  3.833371853894331*^9, 3.8333718546698456`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"-", 
    FractionBox[
     RowBox[{"dq", " ", 
      RowBox[{"Sin", "[", "\[Theta]", "]"}], " ", 
      SubscriptBox["K", "0"], " ", 
      SubscriptBox["Q", "0"]}], 
     SuperscriptBox["R", "2"]]}], ",", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"dq", " ", 
      RowBox[{"Cos", "[", "\[Theta]", "]"}], " ", 
      SubscriptBox["K", "0"], " ", 
      SubscriptBox["Q", "0"]}], 
     SuperscriptBox["R", "2"]]}]}], "}"}]], "Output",
 CellChangeTimes->{3.833381712043083*^9, 3.833383416022504*^9, 
  3.833569492616424*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FQ", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"Q", " ", 
       RowBox[{"Sin", "[", "\[Theta]", "]"}], " ", 
       SubscriptBox["K", "0"], " ", 
       SubscriptBox["Q", "0"]}], 
      RowBox[{"\[Pi]", " ", 
       SuperscriptBox["R", "2"]}]]}], ",", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"Q", " ", 
       RowBox[{"Cos", "[", "\[Theta]", "]"}], " ", 
       SubscriptBox["K", "0"], " ", 
       SubscriptBox["Q", "0"]}], 
      RowBox[{"\[Pi]", " ", 
       SuperscriptBox["R", "2"]}]]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.8333719397129965`*^9, 3.833371971632606*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"-", 
    FractionBox[
     RowBox[{"Q", " ", 
      RowBox[{"Sin", "[", "\[Theta]", "]"}], " ", 
      SubscriptBox["K", "0"], " ", 
      SubscriptBox["Q", "0"]}], 
     RowBox[{"\[Pi]", " ", 
      SuperscriptBox["R", "2"]}]]}], ",", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"Q", " ", 
      RowBox[{"Cos", "[", "\[Theta]", "]"}], " ", 
      SubscriptBox["K", "0"], " ", 
      SubscriptBox["Q", "0"]}], 
     RowBox[{"\[Pi]", " ", 
      SuperscriptBox["R", "2"]}]]}]}], "}"}]], "Output",
 CellChangeTimes->{{3.8333719467955723`*^9, 3.8333719721739583`*^9}, 
   3.833381712491376*^9, 3.8333834161006365`*^9, 3.8335694927845335`*^9}]
}, Open  ]],

Cell[TextData[{
 "Definimos al diferencial de carga ",
 Cell[BoxData[
  FormBox["dq", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", teniendo en cuenta que ",
 Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[
     RowBox[{"dq", "=", 
      RowBox[{
       SubscriptBox["\[Lambda]", "s"], "ds"}]}], TraditionalForm]],
    FormatType->"TraditionalForm"]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y que ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ds", "=", "Rd\[Theta]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", de modo que ",
 Cell[BoxData[
  FormBox[
   RowBox[{"dq", "=", 
    RowBox[{
     SubscriptBox["\[Lambda]", "s"], "Rd\[Theta]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ":"
}], "Text",
 CellChangeTimes->{{3.833371278508029*^9, 3.8333712824429665`*^9}, {
  3.833371381809526*^9, 3.8333714546400747`*^9}, {3.833371620422521*^9, 
  3.833371696004567*^9}},
 TextJustification->1.],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"dq", "=", 
  FractionBox["Q", 
   RowBox[{"\[Pi]", " "}]]}]], "Input",
 CellChangeTimes->{{3.833371457492302*^9, 3.8333714594453397`*^9}, 
   3.833371546090926*^9, {3.833371593144456*^9, 3.8333716170954423`*^9}}],

Cell[BoxData[
 FractionBox["Q", "\[Pi]"]], "Output",
 CellChangeTimes->{3.8333717836073723`*^9, 3.83338171262647*^9, 
  3.8333834161318893`*^9, 3.8335694928365707`*^9}]
}, Open  ]],

Cell[TextData[{
 "Finalmente, integramos la fuerza obtenida respecto al diferencial de \
\[AAcute]ngulo ",
 Cell[BoxData[
  FormBox["d\[Theta]", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ":"
}], "Text",
 CellChangeTimes->{{3.8333717196998663`*^9, 3.8333717811795363`*^9}, {
  3.833371824667095*^9, 3.833371825523662*^9}},
 TextJustification->1.],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{"FQ", ",", 
    RowBox[{"{", 
     RowBox[{"\[Theta]", ",", 
      RowBox[{"-", 
       FractionBox["\[Pi]", "2"]}], ",", 
      FractionBox["\[Pi]", "2"]}], "}"}]}], "]"}], "//", 
  "FullSimplify"}]], "Input",
 CellChangeTimes->{{3.833371712729454*^9, 3.833371714648864*^9}, {
  3.833371836422567*^9, 3.83337193191346*^9}, {3.8333719796416492`*^9, 
  3.833371980252161*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"2", " ", "Q", " ", 
      SubscriptBox["K", "0"], " ", 
      SubscriptBox["Q", "0"]}], 
     RowBox[{"\[Pi]", " ", 
      SuperscriptBox["R", "2"]}]]}]}], "}"}]], "Output",
 CellChangeTimes->{3.8333719330462093`*^9, 3.833371980871567*^9, 
  3.8333817139022894`*^9, 3.8333834162412653`*^9, 3.8335694941528435`*^9}]
}, Open  ]],


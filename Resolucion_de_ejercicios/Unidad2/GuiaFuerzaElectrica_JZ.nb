Cell[BoxData[{"\"\<(a) Distancia de separaci\[OAcute]n entre las cargas\>\"", \
"\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Refine", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{"2", "x"}], "]"}], "/.", 
     RowBox[{"First", "[", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{"b", "\[Equal]", 
         FractionBox[
          SuperscriptBox["x", "2"], "b"]}], ",", "x"}], "]"}], "]"}]}], ",", 
    RowBox[{"b", ">", "0"}]}], "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Sumatoria", " ", "de", " ", "fuerzas", " ", "en", " ", "la", " ", "carga",
     " ", "izquierda"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalSigma]F", "=", 
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"-", "Fel"}], "+", 
          RowBox[{"Fn", " ", 
           RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], "\[Equal]", "0"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"-", "m"}], " ", "g"}], "+", 
          RowBox[{"Fn", " ", 
           RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "\[Equal]", "0"}]}], 
       "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"Fel", ",", "Fn"}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Magnitudes", " ", "de", " ", "las", " ", "fuerzas", " ", 
    "el\[EAcute]ctricas"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Fel1", "=", 
   RowBox[{"Fel", "/.", 
    RowBox[{"\[CapitalSigma]F", "[", 
     RowBox[{"[", "1", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Fel2", "=", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{"q", ">", "0"}], ",", 
      RowBox[{"Refine", "[", 
       RowBox[{"MFe", "[", 
        RowBox[{"q", ",", "q", ",", 
         RowBox[{"2", "b"}], ",", 
         SubscriptBox["K", "0"]}], "]"}], "]"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Carga", " ", "el\[EAcute]ctrica", " ", "sin", " ", "valor", " ", "de", 
    " ", "la", " ", 
    RowBox[{"Cot", "[", "\[Theta]", "]"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Carga", "=", 
    RowBox[{"q", "/.", 
     RowBox[{"Last", "[", 
      RowBox[{"Assuming", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"g", ">", "0"}], "&&", 
         RowBox[{"m", ">", "0"}], "&&", 
         RowBox[{"\[ScriptL]", ">", "0"}], "&&", 
         RowBox[{
          SubscriptBox["K", "0"], ">", "0"}], "&&", 
         RowBox[{"q", ">", "0"}]}], ",", 
        RowBox[{"FullSimplify", "[", 
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{"Fel1", "\[Equal]", "Fel2"}], ",", "q"}], "]"}], "]"}]}], 
       "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Valor", " ", "de", " ", "y", " ", "dada", " ", "la", " ", 
    "ecuaci\[OAcute]n", " ", "de", " ", "la", " ", "curva"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"y", "=", 
    FractionBox[
     SuperscriptBox["x", "2"], "b"]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Derivada", " ", "de", " ", "y", " ", "para", " ", "hallar", " ", "la", 
    " ", "pendiente", " ", "de", " ", "la", " ", "recta", " ", "tangente", 
    " ", "a", " ", "la", " ", "curva"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Ty", "=", 
    RowBox[{"D", "[", 
     RowBox[{"y", ",", "x"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Pendiente", " ", "de", " ", "la", " ", "recta", " ", "normal", " ", "a", 
    " ", "la", " ", "tangente", " ", "y", " ", "valor", " ", "de", " ", "la", 
    " ", 
    RowBox[{"Tan", "[", "\[Theta]", "]"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Ny", "=", 
    RowBox[{"-", 
     FractionBox["1", "Ty"]}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Cuando", " ", "se", " ", "sustituye", " ", "la", " ", 
     RowBox[{"Tan", "[", "\[Theta]", "]"}], " ", "en", " ", "el", " ", 
     "valor", " ", "de", " ", "la", " ", "carga"}], ",", " ", 
    RowBox[{
    "se", " ", "cambia", " ", "el", " ", "valor", " ", "de", " ", "x", " ", 
     "por", " ", "el", " ", "de", " ", "la", " ", "componente", " ", 
     "horizontal", " ", "de", " ", "la", " ", "posici\[OAcute]n", " ", "de", 
     " ", "la", " ", "carga"}]}], 
   "*)"}]}], "\[IndentingNewLine]", "\"\<(b) Carga el\[EAcute]ctrica\>\"", "\
\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Carga", "/.", 
   RowBox[{
    RowBox[{"Cot", "[", "\[Theta]", "]"}], "\[Rule]", 
    FractionBox["1", "Ny"]}]}], "/.", 
  RowBox[{"x", "\[Rule]", 
   RowBox[{"-", "b"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "\[CapitalSigma]F", ",", "Fel1", ",", "Fel2", ",", "Carga", ",", "y", ",", 
    "Ty", ",", "Ny"}], "]"}], ";"}]}], "Input",
 CellChangeTimes->{{3.833971719127796*^9, 3.833971752617448*^9}, {
  3.833971821177967*^9, 3.8339719358948374`*^9}, {3.833971966696161*^9, 

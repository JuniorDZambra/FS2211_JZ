Cell[TextData[{
 StyleBox["10. ",
  FontWeight->"Bold"],
 "Una carga ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "=", 
    RowBox[{"5", "e"}]}], TraditionalForm]]],
 " se encuentra fija en el origen de un sistema de coordenada. A una \
distancia horizontal ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]]],
 " y a la derecha de ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]]],
 " se coloca otra carga ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "=", 
    RowBox[{
     RowBox[{"-", "3"}], "e"}]}], TraditionalForm]]],
 ", la cual puede moverse libremente sobre el eje horizontal. Este sistema se \
encuentra inmerso en una regi\[OAcute]n de campo el\[EAcute]ctrico uniforme ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "0"], TraditionalForm]]],
 " que mantiene en reposo a la carga ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]]],
 " a la distancia ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]]],
 ", respecto de ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]]],
 ", El vector campo el\[EAcute]ctrico viene dado por:"
}], "Text",
 CellChangeTimes->{{3.8339953240478334`*^9, 3.8339953391905327`*^9}, {
  3.833995445632256*^9, 3.833995500749646*^9}, {3.8340616172080507`*^9, 
  3.8340616178074465`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Magnitud", " ", "de", " ", "la", " ", "fuerza", " ", "el\[EAcute]ctrica", 
    " ", "entre", " ", "las", " ", "cargas", " ", "puntuales"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Fel", "=", 
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"e", ">", "0"}], "&&", 
        RowBox[{"d", ">", "0"}]}], ",", 
       RowBox[{"Refine", "[", 
        RowBox[{"MFe", "[", 
         RowBox[{
          RowBox[{"5", "e"}], ",", 
          RowBox[{
           RowBox[{"-", "3"}], "e"}], ",", "d", ",", 
          SubscriptBox["K", "0"]}], "]"}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Sumatoria", " ", "de", " ", "fuerzas", " ", "sobre", " ", "la", " ", 
     "carga", " ", "derecha"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Asumimos", " ", "que", " ", "la", " ", "magnitud", " ", "de", " ", 
      SubscriptBox["E", "0"], " ", "es", " ", "positiva", " ", "y", " ", "su",
       " ", "direcci\[OAcute]n", " ", "es"}], " ", "-", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["e", "^"], "x"], " ", "para", " ", "que", " ", "se", " ",
       "cumpla", " ", "el", " ", "equilibrio"}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[CapitalSigma]F", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"-", "Fel"}], "+", 
         RowBox[{"3", "e", " ", "E0"}]}], "\[Equal]", "0"}], ",", "E0"}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<Vector del campo el\[EAcute]ctrico\>\"", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"E0", "/.", 
      RowBox[{"\[CapitalSigma]F", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ")"}], 
    RowBox[{"{", 
     RowBox[{"1", ",", "0"}], "}"}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{"Fel", ",", "\[CapitalSigma]F"}], "]"}], ";"}]}]}]], "Input",
 CellChangeTimes->{{3.8340615702955055`*^9, 3.8340615729072328`*^9}, {
   3.8340616869301596`*^9, 3.8340617278071938`*^9}, {3.834061815666872*^9, 
   3.834062063099221*^9}, {3.8340621316796317`*^9, 3.834062131758686*^9}, 

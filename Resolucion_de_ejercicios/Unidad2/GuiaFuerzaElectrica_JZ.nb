Cell[TextData[{
 "Recordemos que ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ds", "=", 
    RowBox[{"\[ScriptV]", " ", "d\[Theta]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", donde ",
 Cell[BoxData[
  FormBox["\[ScriptV]", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " es la magnitud de la derivada de la posici\[OAcute]n del diferencial de \
carga. De esa relaci\[OAcute]n obtenemos ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d\[Theta]", "ds"], "=", 
    FractionBox["1", "\[ScriptV]"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". Tenemos que la densidad de carga por regla de la cadena es:"
}], "Text",
 CellChangeTimes->{{3.8338744982765737`*^9, 3.833874501987508*^9}, {
  3.8338745522901783`*^9, 3.83387493188597*^9}, {3.833875186228771*^9, 
  3.8338751932344694`*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  StyleBox[
   RowBox[{
    RowBox[{"\[Lambda]", 
     RowBox[{"(", "s", ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      FractionBox["dq", "d\[Theta]"], " ", 
      FractionBox["d\[Theta]", "ds"]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Lambda]", "*"], 
       FractionBox["d\[Theta]", "ds"]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["\[Lambda]", "*"], 
         FractionBox["1", "\[ScriptV]"]}], "\[Implies]", 
        RowBox[{"\[Lambda]", "(", "s", ")"}]}], "=", 
       RowBox[{"Q", " ", 
        RowBox[{"Sin", "[", "\[Theta]", "]"}], 
        FractionBox["1", "\[ScriptV]"]}]}]}]}]}], "InlineFormula"], 
  TraditionalForm]]]], "Text",
 CellChangeTimes->{3.8338751887778454`*^9},
 TextAlignment->Center],


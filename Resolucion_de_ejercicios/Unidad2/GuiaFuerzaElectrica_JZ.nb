Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Sumatoria", " ", "de", " ", "fuerzas", " ", "sobre", " ", "la", " ", 
    "carga", " ", "izquierda"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[CapitalSigma]F", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"Fel", "-", 
           RowBox[{"T", " ", 
            RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "\[Equal]", "0"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"-", "m"}], " ", "g"}], "+", 
           RowBox[{"T", " ", 
            RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], "\[Equal]", "0"}]}], 
        "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Fel", ",", "T"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Magnitudes", " ", "de", " ", "la", " ", "fuerza", " ", 
     "el\[EAcute]ctrica"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fel1", "=", 
     RowBox[{"Fel", "/.", 
      RowBox[{"\[CapitalSigma]F", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fel2", "=", 
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{"Q", ">", "0"}], ",", 
       RowBox[{"Refine", "[", 
        RowBox[{"MFe", "[", 
         RowBox[{"Q", ",", 
          RowBox[{"-", "Q"}], ",", "\[ScriptL]", " ", ",", 
          SubscriptBox["K", "0"]}], "]"}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\"\<(a) \[CapitalAAcute]ngulo \[Theta]\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"Angulo", "=", 
    RowBox[{
     RowBox[{"\[Theta]", "-", 
      RowBox[{"\[Pi]", " ", 
       RowBox[{"C", "[", "1", "]"}]}]}], "/.", 
     RowBox[{"Normal", "[", 
      RowBox[{"First", "[", 
       RowBox[{"Assuming", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"g", ">", "0"}], "&&", 
          RowBox[{"m", ">", "0"}], "&&", 
          RowBox[{"\[ScriptL]", ">", "0"}], "&&", 
          RowBox[{
           SubscriptBox["K", "0"], ">", "0"}], "&&", 
          RowBox[{"q", ">", "0"}], "&&", 
          RowBox[{
           FractionBox["\[Pi]", "2"], ">", "\[Theta]", ">", "0"}]}], ",", 
         RowBox[{"FullSimplify", "[", 
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{"Fel1", "\[Equal]", "Fel2"}], ",", "\[Theta]"}], "]"}], 
          "]"}]}], "]"}], "]"}], "]"}]}]}], "\[IndentingNewLine]", 
   RowBox[{"First", "[", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"g", ">", "0"}], "&&", 
       RowBox[{"m", ">", "0"}], "&&", 
       RowBox[{"\[ScriptL]", ">", "0"}], "&&", 
       RowBox[{
        SubscriptBox["K", "0"], ">", "0"}], "&&", 
       RowBox[{"q", ">", "0"}], "&&", 
       RowBox[{
        FractionBox["\[Pi]", "2"], ">", "\[Theta]", ">", "0"}]}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"x", "\[Equal]", 
          RowBox[{"\[ScriptL]", " ", 
           RowBox[{"Sin", "[", "Angulo", "]"}]}]}], ",", "x"}], "]"}], 
       "]"}]}], "]"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"First", "[", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"g", ">", "0"}], "&&", 
       RowBox[{"m", ">", "0"}], "&&", 
       RowBox[{"\[ScriptL]", ">", "0"}], "&&", 
       RowBox[{
        SubscriptBox["K", "0"], ">", "0"}], "&&", 
       RowBox[{"q", ">", "0"}], "&&", 
       RowBox[{
        FractionBox["\[Pi]", "2"], ">", "\[Theta]", ">", "0"}]}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"y", "\[Equal]", 
          RowBox[{"\[ScriptL]", " ", "-", 
           RowBox[{"\[ScriptL]", " ", 
            RowBox[{"Cos", "[", "Angulo", "]"}]}]}]}], ",", "y"}], "]"}], 
       "]"}]}], "]"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 

Cell[TextData[{
 StyleBox["(a)",
  FontWeight->"Bold"],
 " Determine cuanto se desplaza cada bloque despu\[EAcute]s de adquirir la \
carga ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".\n",
 StyleBox["(b)",
  FontWeight->"Bold"],
 " Obtenga una expresi\[OAcute]n para la carga ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " en funci\[OAcute]n de los datos."
}], "Text",
 CellChangeTimes->{{3.8339665111958666`*^9, 3.833966538535838*^9}}],

Cell[TextData[StyleBox["Soluci\[OAcute]n:",
 FontWeight->"Bold",
 Background->RGBColor[0.87, 0.94, 1]]], "Text",
 CellChangeTimes->{3.8338007734988766`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Posici\[OAcute]n", " ", "inicial", " ", "del", " ", "bloque", " ", 
    "derecho", " ", "con", " ", "origen", " ", "de", " ", "coordenadas", " ", 
    "en", " ", "el", " ", "punto", " ", "medio", " ", "de", " ", "la", " ", 
    "distancia", " ", "que", " ", "separa", " ", "ambos", " ", "bloques"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ri", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       FractionBox[
        SubscriptBox["\[ScriptL]", "0"], "2"]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Posici\[OAcute]n", " ", "final"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rf", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       SubscriptBox["\[ScriptL]", "0"]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", "\"\<(a) Diferencia de distancias\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[ScriptL]", "0"], ">", "0"}], ",", 
     RowBox[{"Refine", "[", 
      RowBox[{"Norm", "[", 
       RowBox[{"rf", "-", "ri"}], "]"}], "]"}]}], "]"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Sumatoria", " ", "de", " ", "fuerzas", " ", "sobre", " ", "el", " ", 
     "bloque", " ", "derecho"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[CapitalSigma]F", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Fel", "-", 
         RowBox[{
          SubscriptBox["k", "0"], 
          FractionBox[
           SubscriptBox["\[ScriptL]", "0"], "2"]}]}], "\[Equal]", "0"}], ",", 
       "Fel"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Magnitudes", " ", "de", " ", "las", " ", "fuerzas", " ", 
     "el\[EAcute]ctricas"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fel1", "=", 
     RowBox[{"Fel", "/.", 
      RowBox[{"\[CapitalSigma]F", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fel2", "=", 
     RowBox[{
      RowBox[{"Assuming", "[", 
       RowBox[{
        RowBox[{"Q", ">", "0"}], ",", 
        RowBox[{"Refine", "[", 
         RowBox[{"MFe", "[", 
          RowBox[{"Q", ",", "Q", ",", 
           RowBox[{"2", 
            SubscriptBox["\[ScriptL]", "0"]}], ",", 
           SubscriptBox["K", "0"]}], "]"}], "]"}]}], "]"}], "/.", 
      RowBox[{
       SubscriptBox["K", "0"], "\[Rule]", 
       FractionBox["1", 
        RowBox[{"4", "\[Pi]", " ", 
         SubscriptBox["\[Epsilon]", "0"]}]]}]}]}], ";"}], 
   "\[IndentingNewLine]", "\"\<(b) Valor de la carga el\[EAcute]ctrica\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"Last", "[", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[ScriptL]", "0"], ">", "0"}], "&&", 
       RowBox[{
        SubscriptBox["k", "0"], ">", "0"}], "&&", 
       RowBox[{
        SubscriptBox["\[Epsilon]", "0"], ">", "0"}]}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"Fel1", "\[Equal]", "Fel2"}], ",", "Q"}], "]"}], "]"}]}], 
     "]"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{
     "ri", ",", "rf", ",", "\[CapitalSigma]F", ",", "Fel1", ",", "Fel2"}], 
     "]"}], ";"}]}]}]], "Input",
 CellChangeTimes->{{3.833966541741453*^9, 3.8339665501574783`*^9}, {
   3.8339665844077387`*^9, 3.8339669590258865`*^9}, {3.8339669997305202`*^9, 
   3.8339672446558495`*^9}, {3.8339789412179956`*^9, 3.8339789735143766`*^9}, 
   3.834253410417342*^9}],

Cell[BoxData["\<\"(a) Diferencia de distancias\"\>"], "Output",
 CellChangeTimes->{3.8339789754339375`*^9, 3.8339895221369*^9}],

Cell[BoxData[
 FractionBox[
  SubscriptBox["\[ScriptL]", "0"], "2"]], "Output",
 CellChangeTimes->{3.8339789754339375`*^9, 3.8339895221369*^9}],

Cell[BoxData["\<\"(b) Valor de la carga el\[EAcute]ctrica\"\>"], "Output",
 CellChangeTimes->{3.8339789754339375`*^9, 3.8339895221369*^9}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"Q", "\[Rule]", 
   RowBox[{"2", " ", 
    SqrtBox[
     RowBox[{"2", " ", "\[Pi]"}]], " ", 
    SqrtBox[
     RowBox[{
      SubscriptBox["k", "0"], " ", 
      SubsuperscriptBox["\[ScriptL]", "0", "3"], " ", 
      SubscriptBox["\[Epsilon]", "0"]}]]}]}], "}"}]], "Output",
 CellChangeTimes->{3.8339789754339375`*^9, 3.8339895222150307`*^9}]

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Posici\[OAcute]n", " ", "de", " ", "las", " ", "cargas", " ", 
    "puntuales"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rq1", "=", 
     RowBox[{"{", 
      RowBox[{

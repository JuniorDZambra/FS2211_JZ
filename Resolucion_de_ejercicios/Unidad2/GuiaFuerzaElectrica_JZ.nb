  RowBox[{"0", ",", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"128", " ", 
      SuperscriptBox["q", "2"], " ", 
      SubscriptBox["K", "0"]}], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "4"}], "+", 

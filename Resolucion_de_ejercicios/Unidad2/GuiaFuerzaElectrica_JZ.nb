Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Sumatoria", " ", "de", " ", "fuerzas", " ", "sobre", " ", "la", " ", 
    "carga", " ", "en", " ", "el", " ", "hilo"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[CapitalSigma]F", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"Fel", "-", 
           RowBox[{"T", " ", 
            RowBox[{"Cos", "[", "\[Phi]", "]"}]}]}], "\[Equal]", "0"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"-", "m"}], " ", "g"}], "+", 
           RowBox[{"T", " ", 
            RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], "\[Equal]", "0"}]}], 
        "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Fel", ",", "T"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Magnitudes", " ", "de", " ", "la", " ", "fuerza", " ", 
     "el\[EAcute]ctrica"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fel1", "=", 
     RowBox[{"Fel", "/.", 
      RowBox[{"\[CapitalSigma]F", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fel2", "=", 
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{"Q", ">", "0"}], ",", 
       RowBox[{"Refine", "[", 
        RowBox[{"MFe", "[", 
         RowBox[{"Q", ",", 
          RowBox[{"-", "Q"}], ",", "\[ScriptL]", ",", 
          SubscriptBox["K", "0"]}], "]"}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Valor", " ", "de", " ", 
     RowBox[{"Cot", "[", "\[Phi]", "]"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Ctg", "=", 
     RowBox[{
      RowBox[{"Cot", "[", "\[Phi]", "]"}], "/.", 
      RowBox[{"First", "[", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"Fel2", "\[Equal]", "Fel1"}], ",", 
         RowBox[{"Cot", "[", "\[Phi]", "]"}]}], "]"}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", "\"\<(1a) Valor de Tan[\[Phi]]\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"Tgt", "=", 
    RowBox[{
     RowBox[{"Tan", "[", "\[Phi]", "]"}], "/.", 
     RowBox[{"First", "[", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{
         FractionBox["1", "Ctg"], "\[Equal]", 
         RowBox[{"Tan", "[", "\[Phi]", "]"}]}], ",", 
        RowBox[{"Tan", "[", "\[Phi]", "]"}]}], "]"}], "]"}]}]}], 
   "\[IndentingNewLine]", "\"\<(1b) Valor de T\>\"", "\[IndentingNewLine]", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"g", ">", "0"}], "&&", 
      RowBox[{"m", ">", "0"}], "&&", 
      RowBox[{"\[ScriptL]", ">", "0"}], "&&", 
      RowBox[{
       SubscriptBox["K", "0"], ">", "0"}], "&&", 
      RowBox[{"Q", ">", "0"}]}], ",", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{"T", "/.", 
        RowBox[{"\[CapitalSigma]F", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "/.", 
       RowBox[{"\[Phi]", "\[Rule]", 
        RowBox[{"ArcTan", "[", "Tgt", "]"}]}]}], "]"}]}], "]"}], 
   "\[IndentingNewLine]", "\"\<(1c) Valor de L en cm\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"g", ">", "0"}], "&&", 
       RowBox[{"m", ">", "0"}], "&&", 
       RowBox[{"\[ScriptL]", ">", "0"}], "&&", 
       RowBox[{
        SubscriptBox["K", "0"], ">", "0"}], "&&", 
       RowBox[{"Q", ">", "0"}]}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"L", "/.", 
        RowBox[{"First", "[", 
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"L", " ", 
               RowBox[{"Cos", "[", "\[Phi]", "]"}]}], "/.", 
              RowBox[{"\[Phi]", "\[Rule]", 
               RowBox[{"ArcTan", "[", "Tgt", "]"}]}]}], ")"}], "\[Equal]", 
            RowBox[{"2", "\[ScriptL]"}]}], ",", "L"}], "]"}], "]"}]}], 
       "]"}]}], "]"}], "\[Times]", 
    FractionBox["1", "100"]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{
     "\[CapitalSigma]F", ",", "Fel1", ",", "Fel2", ",", "Ctg", ",", "Tgt", 
      ",", "T", ",", "L"}], "]"}], ";"}]}]}]], "Input",

Cell["\<\
Sobre la base de este planteamiento responda las siguientes preguntas:\
\>", "Text",
 CellChangeTimes->{3.8338946056366577`*^9}],

Cell[TextData[{
 StyleBox["(2a) ",
  FontWeight->"Bold"],
 "Obtenga la fuerza el\[EAcute]ctrica sobre la carga ubicada en el origen del \
sistema de coordenadas.\n",
 StyleBox["(2b)",
  FontWeight->"Bold"],
 " Si las part\[IAcute]culas de carga ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", "q"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y ",
 Cell[BoxData[
  FormBox[
   RowBox[{"3", "q"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " presentan igual masa, calcule la fuerza el\[EAcute]ctrica sobre la carga \
ubicada en el origen suponiendo que la carga neta de las otras dos part\
\[IAcute]culas est\[AAcute] ubicada en su centro de masa.\n",
 StyleBox["(2c)",
  FontWeight->"Bold"],
 " Si las part\[IAcute]culas de carga ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", "q"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y ",
 Cell[BoxData[
  FormBox[
   RowBox[{"3", "q"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " presentan igual relaci\[OAcute]n de masa (es decir, ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y ",
 Cell[BoxData[
  FormBox[
   RowBox[{"3", "m"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "), calcule la fuerza el\[EAcute]ctrica sobre la carga ubicada en el origen \
suponiendo que la carga neta de las otras dos part\[IAcute]culas est\[AAcute] \
ubicada en su centro de masa."
}], "Text",
 CellChangeTimes->{{3.833894619096438*^9, 3.8338947005289297`*^9}}],

Cell[TextData[StyleBox["Soluci\[OAcute]n:",
 FontWeight->"Bold",
 Background->RGBColor[0.87, 0.94, 1]]], "Text",
 CellChangeTimes->{3.8338007734988766`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{"Posiciones", " ", "de", " ", "las", " ", "cargas"}], " ", "-", 
     "q"}], ",", " ", 
    RowBox[{"2", "q", " ", "y", " ", "3", "q"}], ",", " ", 
    "respectivamente"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rq1", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "3"}], "a"}], ",", 
       RowBox[{"4", "a"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq2", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq3", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"4", "a"}], ",", 
       RowBox[{"3", "a"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<(2a) Fuerza el\[EAcute]ctrica sobre 2q\>\"", "\[IndentingNewLine]", 
   RowBox[{"FullSimplify", "[", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{"a", ">", "0"}], ",", 
      RowBox[{"Refine", "[", 
       RowBox[{
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{"2", "q"}], ",", "rq2", ",", 
          RowBox[{"-", "q"}], ",", "rq1"}], "]"}], "+", 
        RowBox[{"Fe", "[", 
         RowBox[{
          RowBox[{"2", "q"}], ",", "rq2", ",", 
          RowBox[{"3", "q"}], ",", "rq3"}], "]"}]}], "]"}]}], "]"}], "]"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Carga", " ", "neta", " ", "de", " ", "las", " ", "part\[IAcute]culas", 
      " ", "de", " ", "carga"}], " ", "-", 
     RowBox[{"q", " ", "y", " ", "3", "q"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Q", "=", 
     RowBox[{
      RowBox[{"-", "q"}], "+", 
      RowBox[{"3", "q"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Posici\[OAcute]n", " ", "del", " ", "centro", " ", "de", " ", "masa", 
      " ", "de", " ", "las", " ", "part\[IAcute]culas", " ", "con", " ", 
      "carga"}], " ", "-", 
     RowBox[{
     "q", " ", "y", " ", "3", "q", " ", "e", " ", "igual", " ", "masa", " ", 
      "m"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rcm1", "=", 
     FractionBox[
      RowBox[{
       RowBox[{"m", " ", "rq1"}], "+", 
       RowBox[{"m", " ", "rq2"}]}], 
      RowBox[{"m", "+", "m"}]]}], ";"}], "\[IndentingNewLine]", 
   "\"\<(2b) Fuerza el\[EAcute]ctrica sobre 2q debido a la carga especificada\
\>\"", "\[IndentingNewLine]", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{"a", ">", "0"}], ",", 
     RowBox[{"Refine", "[", 
      RowBox[{"Fe", "[", 
       RowBox[{
        RowBox[{"2", "q"}], ",", "rq2", ",", "Q", ",", "rcm1"}], "]"}], 
      "]"}]}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{

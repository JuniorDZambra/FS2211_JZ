      SuperscriptBox["10", "9"]}]}], "]"}], ":=", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"K", ">", "0"}], "&&", 
      RowBox[{"d", ">", "0"}]}], ",", 
     RowBox[{"Refine", "[", 
      RowBox[{"Abs", "[", 
       RowBox[{"K", 
        FractionBox[

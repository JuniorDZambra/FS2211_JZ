Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Fe", "[", 
    RowBox[{"qf_", ",", "rf_", ",", "qi_", ",", "ri_"}], "]"}], ":=", 
   RowBox[{
    SubscriptBox["K", "0"], 
    FractionBox[
     RowBox[{"qf", " ", "qi"}], 
     SuperscriptBox[
      RowBox[{"Norm", "[", 
       RowBox[{"rf", "-", "ri"}], "]"}], "3"]], 
    RowBox[{"(", 

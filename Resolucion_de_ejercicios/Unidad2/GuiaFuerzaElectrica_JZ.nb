Cell[BoxData[
 RowBox[{
  RowBox[{
   StyleBox["(*", "Input"], 
   RowBox[{
    RowBox[{
    "Posici\[OAcute]n", " ", "de", " ", "la", " ", "carga", " ", "puntual", 
     " ", "y", " ", "del", " ", "diferencial", " ", "de", " ", "carga"}], ",",
     " ", "respectivamente"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rq0", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rdq", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"R", " ", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ",", 
       RowBox[{"R", " ", 
        RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    StyleBox["(*", "Input"], 
    RowBox[{
     RowBox[{
     "Diferencial", " ", "de", " ", "carga", " ", "de", " ", "la", " ", 
      "forma", " ", "dq"}], "=", 
     RowBox[{
      RowBox[{"\[LeftAngleBracket]", "\[Lambda]", "\[RightAngleBracket]"}], 
      "rd\[Theta]"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dq", "=", 
     RowBox[{
      FractionBox["Q", 
       FractionBox[
        RowBox[{"2", "\[Pi]", " ", "R"}], "4"]], " ", "R"}]}], ";"}], 
   "\[IndentingNewLine]", "\"\<Diferencial de Fuerza El\[EAcute]ctrica\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"dF", "=", 
    RowBox[{"Cancel", "[", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"Assuming", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"R", ">", "0"}], "&&", 
         RowBox[{
          FractionBox["\[Pi]", "2"], "\[GreaterEqual]", "\[Theta]", 
          "\[GreaterEqual]", "0"}]}], ",", 
        RowBox[{"Refine", "[", 
         RowBox[{"Fe", "[", 
          RowBox[{"q0", ",", "rq0", ",", "dq", ",", "rdq"}], "]"}], "]"}]}], 
       "]"}], "]"}], "]"}]}], "\[IndentingNewLine]", 
   "\"\<Fuerza El\[EAcute]ctrica\>\"", "\[IndentingNewLine]", 
   RowBox[{"F", "=", 
    RowBox[{"Cancel", "[", 
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{"R", ">", "0"}], ",", 
       RowBox[{"Integrate", "[", 
        RowBox[{"dF", ",", 
         RowBox[{"{", 
          RowBox[{"\[Theta]", ",", "0", ",", 
           FractionBox["\[Pi]", "2"]}], "}"}]}], "]"}]}], "]"}], "]"}]}], 
   "\[IndentingNewLine]", "\"\<Factorizando\>\"", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"PolynomialGCD", "@@", "F"}], ",", 
     RowBox[{"F", "/", 
      RowBox[{"PolynomialGCD", "@@", "F"}]}]}], "}"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{"rq0", ",", "rdq", ",", "dq", ",", "dF", ",", "F"}], "]"}], 
    ";"}]}]}]], "Input",

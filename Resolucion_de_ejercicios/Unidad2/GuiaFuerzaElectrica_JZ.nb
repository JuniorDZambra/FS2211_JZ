   RowBox[{
    RowBox[{"rq", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "h"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<(a) Fuerza el\[EAcute]ctrica sobre la esfera\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"F", "=", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"q", ">", "0"}], "&&", 
       RowBox[{"a", ">", "0"}], "&&", 
       RowBox[{"h", ">", "0"}]}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{"Fe", "[", 
         RowBox[{"q", ",", "rq", ",", "q", ",", "rq1"}], "]"}], "+", 
        RowBox[{"Fe", "[", 
         RowBox[{"q", ",", "rq", ",", "q", ",", "rq2"}], "]"}]}], "]"}]}], 
     "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Sumatoria", " ", "de", " ", "fuerzas", " ", "sobre", " ", "la", " ", 
     "esfera", " ", "cuando", " ", "se", " ", "alcanza", " ", "el", " ", 
     "equilibro", " ", "electrost\[AAcute]tico"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"\[CapitalSigma]F", "=", 
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"-", "m"}], " ", "g"}], " ", "+", " ", "Fel"}], "\[Equal]", 
       "0"}], ",", "Fel"}], "]"}]}], "\[IndentingNewLine]", 
   "\"\<(b) Expresi\[OAcute]n para a cuando la esfera alcanza el equilibro \
electrost\[AAcute]tico\>\"", "\[IndentingNewLine]", 
   RowBox[{"Last", "[", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"g", ">", "0"}], "&&", 
       RowBox[{"m", ">", "0"}], "&&", 
       RowBox[{"h", ">", "0"}], "&&", 
       RowBox[{"q", ">", "0"}], "&&", 
       RowBox[{
        SubscriptBox["K", "0"], ">", "0"}], "&&", "Elem"}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"F", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "\[Equal]", 
          RowBox[{"(", 
           RowBox[{"Fel", "/.", 
            RowBox[{"\[CapitalSigma]F", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}], ",", "a"}], "]"}], 
       "]"}]}], "]"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Estudio", " ", "de", " ", "los", " ", "puntos", " ", "cr\[IAcute]ticos", 
     " ", "de", " ", "F"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"PtoCritF", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{"F", ",", "h"}], "]"}], "\[Equal]", "0"}], ",", "h"}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<(c) Valor de h cuando F se hace m\[AAcute]xima\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"PtoCritF", "[", 
    RowBox[{"[", "2", "]"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{

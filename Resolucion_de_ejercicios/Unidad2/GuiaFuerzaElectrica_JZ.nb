Cell[TextData[{
 "M\[EAcute]todo 1: definiendo la posici\[OAcute]n de ",
 Cell[BoxData[
  FormBox["dq", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " tal que su componente horizontal ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", " ", 
    RowBox[{"Cos", "[", "\[Theta]", "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " var\[IAcute]e entre 0 y ",
 Cell[BoxData[
  FormBox[
   FractionBox["\[Pi]", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", su componente vertical ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", " ", 
    RowBox[{"Sin", "[", "\[Theta]", "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " var\[IAcute]e tambi\[EAcute]n 0 y ",
 Cell[BoxData[
  FormBox[
   FractionBox["\[Pi]", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y cuyo diferencial de \[AAcute]rea sea ",
 Cell[BoxData[
  FormBox[
   RowBox[{"dA", "=", "rd\[Theta]dr"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", donde ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " corresponde al \[AAcute]ngulo medido desde la horizontal y ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " el radio del segmento circular que var\[IAcute]a entre ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " y ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.8337999198372087`*^9, 3.8338001114152403`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   StyleBox["(*", "Input"], 
   RowBox[{
    RowBox[{
    "Posici\[OAcute]n", " ", "de", " ", "la", " ", "carga", " ", "puntual", 
     " ", "y", " ", "del", " ", "diferencial", " ", "de", " ", "carga"}], ",",
     " ", "respectivamente"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rq0", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rdq", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"r", " ", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], " ", ",", 
       RowBox[{"r", " ", 
        RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "\[CapitalAAcute]rea", " ", "del", " ", "segmento", " ", "circular", " ",
       "de", " ", "radio", " ", "a", " ", "y", " ", "de", " ", "radio", " ", 
      "b", " ", "indicados", " ", "en", " ", "la", " ", "figura"}], ",", " ", 
     "respectivamente"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Aa", "=", 
     RowBox[{
      FractionBox["\[Pi]", "4"], " ", 
      SuperscriptBox["a", "2"]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Ab", "=", 
     RowBox[{
      FractionBox["\[Pi]", "4"], " ", 
      SuperscriptBox["b", "2"]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    StyleBox["(*", "Input"], 
    RowBox[{
     RowBox[{
     "Diferencial", " ", "de", " ", "carga", " ", "de", " ", "la", " ", 
      "forma", " ", "dq"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"\[LeftAngleBracket]", "\[Lambda]", "\[RightAngleBracket]"}], 
       "dA"}], "=", 
      RowBox[{
       RowBox[{"\[LeftAngleBracket]", "\[Lambda]", "\[RightAngleBracket]"}], 
       "rd\[Theta]dr"}]}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dq", "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["Q", 
        RowBox[{"(", 
         RowBox[{"Aa", "-", "Ab"}], ")"}]]}], "r"}]}], ";"}], 
   "\[IndentingNewLine]", "\"\<Diferencial de Fuerza El\[EAcute]ctrica\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"dF", "=", 
    RowBox[{"Cancel", "[", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{"Fe", "[", 
        RowBox[{"q0", ",", "rq0", ",", "dq", ",", "rdq"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"a", ">", "b", ">", "0"}], ",", 
         RowBox[{"r", ">", "0"}], ",", 
         RowBox[{
          FractionBox["\[Pi]", "2"], "\[GreaterEqual]", "\[Theta]", 
          "\[GreaterEqual]", "0"}]}], "}"}]}], "]"}], "]"}]}], 
   "\[IndentingNewLine]", "\"\<Fuerza El\[EAcute]ctrica\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"F", "=", 
    RowBox[{"Cancel", "[", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"Assuming", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"a", "\[NotEqual]", "b"}], "&&", 
         RowBox[{"a", ">", "b", ">", "0"}]}], ",", 
        RowBox[{"Integrate", "[", 
         RowBox[{"dF", ",", 
          RowBox[{"{", 
           RowBox[{"r", ",", "b", ",", "a"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Theta]", ",", "0", ",", 
            FractionBox["\[Pi]", "2"]}], "}"}]}], "]"}]}], "]"}], "]"}], 
     "]"}]}], "\[IndentingNewLine]", "\"\<Factorizando\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"PolynomialGCD", "@@", "F"}], ",", 
     RowBox[{"F", "/", 
      RowBox[{"PolynomialGCD", "@@", "F"}]}]}], "}"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{
     "rq0", ",", "rdq", ",", "Aa", ",", "Ab", ",", "dq", ",", "dF", ",", 
      "F"}], "]"}], ";"}]}]}]], "Input",
 CellChangeTimes->{{3.8337446580333357`*^9, 3.833744675453211*^9}, 

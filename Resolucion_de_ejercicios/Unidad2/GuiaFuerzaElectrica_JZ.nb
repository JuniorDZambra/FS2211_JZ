Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Sumatoria", " ", "de", " ", "fuerzas", " ", "sobre", " ", "la", " ", 
    "carga", " ", "izquierda"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[CapitalSigma]F", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{

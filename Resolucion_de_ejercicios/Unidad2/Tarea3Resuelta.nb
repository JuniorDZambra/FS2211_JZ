Cell[TextData[{
 StyleBox["(2b)",
  FontWeight->"Bold"],
 " Compare la fuerza calculada en el inciso anterior con la fuerza \
el\[EAcute]ctrica que ejerce el semi-aro si toda su carga est\[AAcute] \
concentrada en el centro de masa del aro. Suponga que la masa del semi-aro se \
distribuye de igual manera que la carga."
}], "Text",
 CellChangeTimes->{{3.8333720187688303`*^9, 3.833372023728015*^9}, {
  3.8333722570599813`*^9, 3.8333722706067095`*^9}},
 TextJustification->1.],

Cell[TextData[{
 "Calculamos la posici\[OAcute]n del centro de masa del semi-aro, teniendo en \
cuenta que su diferencial de masa es ",
 Cell[BoxData[
  FormBox[
   RowBox[{"dm", "=", 
    RowBox[{
     FractionBox["M", 
      RowBox[{"\[Pi]", " "}]], "d\[Theta]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ":"
}], "Text",
 CellChangeTimes->{{3.8333726252916093`*^9, 3.8333726332900496`*^9}, {
  3.833372677371195*^9, 3.83337278548685*^9}},
 TextJustification->1.],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"rcm", "=", 
  RowBox[{
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"{", 
        RowBox[{
         RowBox[{"R", " ", 
          RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
         RowBox[{"R", " ", 
          RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], "}"}], "M"], 
      FractionBox["M", "\[Pi]"]}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Theta]", ",", 
       RowBox[{"-", 
        FractionBox["\[Pi]", "2"]}], ",", 
       FractionBox["\[Pi]", "2"]}], "}"}]}], "]"}], "//", 
   "FullSimplify"}]}]], "Input",
 CellChangeTimes->{{3.83337280002846*^9, 3.833372937064441*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", 
   FractionBox[
    RowBox[{"2", " ", "R"}], "\[Pi]"]}], "}"}]], "Output",
 CellChangeTimes->{{3.8333729143217916`*^9, 3.8333729514966316`*^9}, 
   3.833381714467662*^9, 3.83338341630377*^9, 3.83356949436098*^9}]
}, Open  ]],


Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Posiciones", " ", "de", " ", "las", " ", "cargas", " ", "puntuales"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rq1", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq2", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", "a"}], ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq3", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"2", "b"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rq4", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", "a"}], ",", 
       RowBox[{"2", "b"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rP", "=", 
     RowBox[{"{", 
      RowBox[{"a", ",", "b"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<(a) Campo el\[EAcute]ctrico total sobre P\>\"", "\[IndentingNewLine]", 
   RowBox[{"EeP", "=", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"a", ">", "0"}], "&&", 
       RowBox[{"b", ">", "0"}]}], ",", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{"Ee", "[", 
         RowBox[{"rP", ",", 
          RowBox[{"2", "Q"}], ",", "rq1"}], "]"}], "+", 
        RowBox[{"Ee", "[", 
         RowBox[{"rP", ",", 
          RowBox[{
           RowBox[{"-", "2"}], "Q"}], ",", "rq2"}], "]"}], "+", 
        RowBox[{"Ee", "[", 
         RowBox[{"rP", ",", "Q", ",", "rq3"}], "]"}], "+", 
        RowBox[{"Ee", "[", 
         RowBox[{"rP", ",", 
          RowBox[{"-", "Q"}], ",", "rq3"}], "]"}], "+", 
        RowBox[{"E0", 
         RowBox[{"{", 
          RowBox[{"1", ",", "0"}], "}"}]}]}], "]"}]}], "]"}]}], 
   "\[IndentingNewLine]", 
   "\"\<(b) Fuerza el\[EAcute]ctrica total sobre una carga Q colocada en el \
punto P\>\"", "\[IndentingNewLine]", 
   RowBox[{"FeP", "=", 
    RowBox[{"Q", " ", "EeP"}]}], "\[IndentingNewLine]", 
   "\"\<(c) Magnitud de \!\(\*SubscriptBox[\(E\), \(0\)]\) para que la \
\!\(\*SubscriptBox[\(F\), \(e\)]\) sobre P sea 0 para cualquier carga en ese \
punto\>\"", "\[IndentingNewLine]", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"q", " ", 
       RowBox[{"Assuming", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"a", ">", "0"}], "&&", 
          RowBox[{"b", ">", "0"}], "&&", 
          RowBox[{
           SubscriptBox["K", "0"], ">", "0"}], "&&", 
          RowBox[{"Q", ">", "0"}], "&&", 
          RowBox[{"E0", ">", "0"}]}], ",", 
         RowBox[{"Refine", "[", 
          RowBox[{"Norm", "[", "EeP", "]"}], "]"}]}], "]"}]}], "\[Equal]", 
      "0"}], ",", "E0"}], "]"}], "\[IndentingNewLine]", 
   "\"\<(d) Vector momento dipolar del sistema\>\"", "\[IndentingNewLine]", 
   RowBox[{"p", "=", 
    RowBox[{"Sum", "[", 
     RowBox[{"i", ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"2", "Q", " ", "rq1"}], ",", 
          RowBox[{
           RowBox[{"-", "2"}], "Q", " ", "rq2"}], ",", 
          RowBox[{"Q", " ", "rq3"}], ",", 
          RowBox[{
           RowBox[{"-", "Q"}], " ", "rq4"}]}], "}"}]}], "}"}]}], "]"}]}], 
   "\[IndentingNewLine]", "\"\<Carga neta del sistema\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"QN", "=", 
    RowBox[{
     RowBox[{"2", "Q"}], "-", 
     RowBox[{"2", "Q"}], "+", "Q", "-", "Q"}]}], "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"QN", "\[Equal]", "0"}], ",", 
     "\"\<El vector del momento dipolar del sistema no depende del sistema de \
coord.\>\"", ",", 
     "\"\<El vector del momento dipolar del sistema depende del sistema de \
coord.\>\""}], "]"}], "\[IndentingNewLine]", 
   "\"\<(e) Torque neto sobre el sistema\>\"", "\[IndentingNewLine]", 
   RowBox[{"Cross", "[", 
    RowBox[{"p", ",", "E0"}], "]"}], "\[IndentingNewLine]", 
   "\"\<Aceleraci\[OAcute]n angular del sistema\>\"", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"Clear", "[", 
    RowBox[{
    "rq1", ",", "rq2", ",", "rq3", ",", "rq4", ",", "rP", ",", "EeP", ",", 
     "FeP", ",", "p", ",", "QN"}], "]"}]}]}]], "Input",
